{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "nft_daap/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "nft_daap/dist",
        "outputDirectory": "nft_daap/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/(.*)",
      "dest": "nft_daap/dist/index.html",
      "headers": {
        "Content-Type": "text/html"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/nft_daap/dist/index.html"
    }
  ]
}